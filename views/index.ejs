<%- include('partials/header'); %>
<%- include('partials/nav'); %>
<section class="bg-slider">
  <div id="main-slider" class="carousel slide" data-ride="carousel">
    <ol class="carousel-indicators">
      <li data-target="#main-slider" data-slide-to="0" class="active"></li>
      <li data-target="#main-slider" data-slide-to="1"></li>
      <li data-target="#main-slider" data-slide-to="2"></li>
      <li data-target="#main-slider" data-slide-to="3"></li>
    </ol>
    <div class="carousel-inner">
      <div class="carousel-item active">
        <img src="/images/sliders/s1.jpg" alt="..." class="d-block w-100">
        <div class="carousel-caption d-none d-md-block">
          <h5></h5>
          <p></p>
        </div>
      </div>
      <div class="carousel-item">
        <img src="/images/sliders/s2.jpg" alt="" class="d-block w-100">
        <div class="carousel-caption d-none d-md-block">
          <h5></h5>
          <p></p>
        </div>
      </div>
      <div class="carousel-item">
        <img src="/images/sliders/s3.jpg" alt="" class="d-block w-100">
        <div class="carousel-caption d-none d-md-block">
          <h5></h5>
          <p></p>
        </div>
      </div>
      <div class="carousel-item">
        <img src="/images/sliders/s4.jpg" alt="" class="d-block w-100">
        <div class="carousel-caption d-none d-md-block">
          <h5></h5>
          <p></p>
        </div>
      </div>
    </div>
    <a class="carousel-control-prev" href="#main-slider" role="button" data-slide="prev">
      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
      <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#main-slider" role="button" data-slide="next">
      <span class="carousel-control-next-icon" aria-hidden="true"></span>
      <span class="sr-only">Next</span>
    </a>
  </div>
</section>
<main id="main" class="container-fluid">


  <!-- partners sections -->
  <section id="partners" class="colaboration mt-4 mb-4 card card-body">
    <div class="container-fluid">
      <div class="row mb-4">
        <div class="col-md-6">
          <div class="card">
            <div class="card-body">
              <img src="/images/psd/6.png" class="w-100 img-fluid rounded" alt="Responsive image">
              <p>Hiroshima University, Japan</p>
              <img src="/images/psd/4.png" class="w-100 img-fluid rounded" alt="Responsive image">
              <p>Tottori University, Japan</p>
              <img src="/images/psd/5.png" class="w-100 img-fluid rounded" alt="Responsive image">
              <p>Queensland University of Technology, Australia</p>
            </div>
          </div>
        </div>
        <div class="col-md-6">
          <div class="card start-ups">
            <div class="card-header text-white">
              Notifications
            </div>
            <div class="card-body">
              <marquee width="100%" height="300px" direction="up" height="20px" onmouseover="this.stop();" onmouseout="this.start();">
                <% posts.forEach(function(post) { %>
                <a href="page/<%= post.slug %>" target="_blank"><img src="/images/new.jpg" alt=""> <%= post.title %></a>
                <br>
                <% }); %>
              </marquee>
            </div>
          </div>
        </div>
      </div>
    </div>

  </section>

  <!-- end partners sections -->

