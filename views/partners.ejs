<%- include('partials/header'); %>

<%- include('partials/nav'); %>

<main id="main" class="container-fluid">


  <!-- partners sections -->
  <section id="partners" class="colaboration mt-4 mb-4 card card-body">
    <div class="container-fluid">
      <div class="section-title mt-4">
        <h2>Partners</h2>
      </div>
      <div class="row mb-4">
        <div class="col-md-6">
          <div class="card">
            <div class="card-body">
              <h4 class="col-header">INTERNATIONAL COLLABORATORS</h4>
              <br>
              <% collab.forEach(function(collab) { %>
                  <img src="/uploads/<%=collab.images%>" class="w-100 img-fluid rounded" alt="Responsive image">
                  <p><%=collab.collab_name%></p>
              <% }); %>
            </div>
          </div>
        </div>
        <div class="col-md-6">
          <div class="card start-ups">
            <div class="card-header text-white">
              Start-Ups
            </div>
            <div class="card-body">
              <% startup.forEach(function(startup) { %>
              <a href="<%= startup.link %>" class="card start-ups-content mb-2">
                <div class="row no-gutters">
                  <div class="col-md-4 left-start-ups">
                    <img src="/uploads/<%= startup.image%>" class="card-img" alt="...">
                  </div>
                  <div class="col-md-8 right-start-ups">
                    <div class="card-body">
                      <p><img src="/uploads/<%= startup.logo%>" class="w-25 card-img" alt="..."> <span
                          class="pull-right">SiB Follows <%= startup.years%></span></p>
                      <span class="card-text">
                        <%if(startup.co_founder != ''){%>
                          <br><strong>Co-Founders :</strong> <%=startup.co_founder %> </span>
                        <%}%>
                        <%if(startup.founder != ''){%>
                          <br><strong>Founder :</strong> <%=startup.founder %> </span>
                        <%}%>
                      <br>
                      <span><%= startup.content %></span>
                    </div>
                  </div>
                </div>
              </a>
              <% });%>

            </div>
          </div>
        </div>
      </div>
    </div>

  </section>

  <!-- end partners sections -->




